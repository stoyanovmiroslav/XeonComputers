@using Microsoft.AspNetCore.Identity

@model ParentCategory[]

@inject SignInManager<XeonUser> SignInManager
@inject UserManager<XeonUser> UserManager

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - XeonComputers</title>

    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css" integrity="sha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU" crossorigin="anonymous">
    <link rel="stylesheet" type="text/css" href="/css/reset-css.css">
    <link rel="stylesheet" type="text/css" href="/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>

    <div class="container">
        <partial name="_NavbarPartial.cshtml" />
        <partial name="_CookieConsentPartial" />
        <hr class="my-1 hr-xeon" />

        <partial name="_MiddleXeonSectionPartial.cshtml" />
        <hr class="hr-xeon" />
        <div class="body-content">
            <div class="row mt-3">
                <div class="col-md-3">
                    <partial name="_MenuImetsPartial.cshtml" model="Model"/>
                </div>
                <div class="col-md-9">
                    @RenderBody()
                </div>
            </div>

            <hr class="my-1 hr-xeon mt-3" />
            <partial name="_FooterPartial.cshtml" />
        </div>
    </div>

        <script src="~/js/jquery-3.3.1.min.js" type="text/javascript"></script>
        <script src="~/js/popper.min.js" type="text/javascript"></script>
        <script src="~/js/bootstrap.min.js" type="text/javascript"></script>
        @RenderSection("Scripts", required: false)
</body>
</html>